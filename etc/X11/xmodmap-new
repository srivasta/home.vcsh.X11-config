clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5
 
add    Shift   = Shift_L Shift_R
add    Control = Control_L Control_L Control_R
add    Mod1    = Alt_L Alt_R
add    Mod2    = Super_L
add    Mod3    = Super_R
!add    Mod4    = 0x007F 0x0080
add    Mod5    = Mode_switch ISO_Level3_Shift

